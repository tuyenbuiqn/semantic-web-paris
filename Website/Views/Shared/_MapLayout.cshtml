<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <title>@ViewBag.Title</title>
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    @Scripts.Render("~/bundles/CommonScript")
    @Scripts.Render("~/bundles/PluginScript")
    @Scripts.Render("~/bundles/DoTScript")
    @Scripts.Render("~/bundles/tmpl")
    <script src="~/Scripts/ajax.common.js"></script>
    @{
        var keys = new List<string>
    {
            // OK
            "AIzaSyDgfxaXZG6Tu9UX5fI6dQ4k3zOHsGg-L60",
            // ROOT
            "AIzaSyBa9qnwp_ybemKff5Omw5RZPGtLEmLZzZ4",
            "AIzaSyCj9X6BsS5DwJhGQX9PElUtm6L7Osa4fgA"
        };
        var key = new Random().Next(keys.Count);
        var script = Html.Raw("https://maps.googleapis.com/maps/api/js?v=3.exp&key=" + keys[key] + "&libraries=drawing,places");
    }
    <script src="@script"></script>
    <script src="~/Scripts/markerwithlabel.js"></script>
    @Scripts.Render("~/bundles/GmapContextMenu")
    @Styles.Render("~/bundles/CommonStyle")
    @Styles.Render("~/bundles/DoTStyle")

</head>
<body class="theme-blue">
    <div class="ajax" style="display: none;"></div>
    <div id="divshowmessage" class="cssshowmessage"></div>
    <!--Notification-->
    <span id="doTNotification" style="display: none;"></span>
    <script type="text/x-kendo-template" id="doTErrorTemplate">
        <div class="wrong-pass">
            <img src="/Content/images/dialog-error.png" />
            <h5>#= title #</h5>
            <p>#= message #</p>
        </div>
    </script>
    <script type="text/x-kendo-template" id="doTSuccessTemplate">
        <div class="new-mail">
            <img src="/Content/images/success.png" />
            <h5>#= message #</h5>
        </div>
    </script>
    @*@Html.Partial("_Header")*@
    @*@Html.Partial("_MapMainMenu")*@
    @RenderBody()
</body>
</html>
